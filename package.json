{
  "name": "nextra-codemods",
  "version": "0.1.0",
  "description": "Codemods to help migrate from Nextra v3 to v4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "nextra-codemods": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "nextra",
    "codemod",
    "migration",
    "v3",
    "v4"
  ],
  "author": "Jeff Wainwright",
  "license": "MIT",
  "dependencies": {
    "jscodeshift": "^0.15.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/jscodeshift": "^0.11.6",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^9.25.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.4.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  }
}
